{
    "name": "netpas/url-shortener",
    "description": "Yet Another URL Shortener Service",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.3",
        "php-di/php-di": "^6.0",
        "monolog/monolog": "^2.0",
        "nesbot/carbon": "^2.31",
        "mtdowling/jmespath.php": "^2.5",
        "ramsey/uuid": "^4.0",
        "slim/psr7": "^1.0",
        "slim/slim": "^4.5",
        "predis/predis": "^1.1",
        "illuminate/database": "^7.0",
        "illuminate/events": "^7.0",
        "illuminate/support": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "scripts": {
        "start": "# Please run the command below in your terminal:\n  # APP_STAGE=DEVELOPMENT php -S 0.0.0.0:8080 -t public public/index.php",
        "fix-cs": "php-cs-fixer fix --diff"
    }
}
